{
  "name": "RNChallengeV3",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "test": "jest",
    "lint": "eslint  index.js ./__tests__/** ./scripts/*.js  ./src/** --ext .js,.ts,.tsx",
    "start": "npx react-native start --reset-cache",
    "format": "prettier --write **/*.{ts,tsx,js,json,md}",
    "splash": "yarn react-native generate-bootsplash splash/splash.png --background-color=FFFFFF --logo-width=200  --assets-path=assets --flavor=main",
    "prepare": "husky install",
    "ios": "node ./scripts/ios.js env/.dev",
    "type:check": "yarn tsc --noEmit --skipLibCheck",
    "android": "node ./scripts/android.js env/.dev devDebug",
    "postinstall": "node ./scripts/setup.js"
  },
  "lint-staged": {
    "*.{ts,tsx,js}": [
      "eslint  --ext"
    ]
  },
  "dependencies": {
    "@react-native-community/masked-view": "^0.1.11",
    "@react-navigation/native": "^6.0.10",
    "@react-navigation/stack": "^6.2.1",
    "react": "17.0.2",
    "react-fast-compare": "^3.2.0",
    "react-native": "0.68.2",
    "@shopify/react-native-skia": "^0.1.133",
    "react-native-blurhash": "^1.1.10",
    "react-native-bootsplash": "^4.1.5",
    "react-native-config": "^1.4.5",
    "react-native-fast-image": "^8.5.11",
    "react-native-gesture-handler": "^2.4.2",
    "react-native-linear-gradient": "^2.5.6",
    "react-native-reanimated": "^2.8.0",
    "react-native-safe-area-context": "^4.2.5",
    "react-native-screens": "^3.13.1",
    "react-native-svg": "^12.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@commitlint/cli": "^16.2.1",
    "@commitlint/config-conventional": "^16.2.1",
    "@react-native-community/eslint-config": "^2.0.0",
    "@types/jest": "^26.0.23",
    "@types/react-native": "^0.67.3",
    "@types/react-test-renderer": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "babel-jest": "^26.6.3",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "eslint": "^7.32.0",
    "eslint-plugin-import": "^2.22.1",
    "husky": "^7.0.4",
    "jest": "^26.6.3",
    "lint-staged": "^12.2.2",
    "metro-react-native-babel-preset": "^0.67.0",
    "patch-package": "^6.4.7",
    "prettier": "^2.3.0",
    "react-test-renderer": "17.0.2",
    "typescript": "^4.4.4"
  },
  "resolutions": {
    "@types/react": "^17"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
